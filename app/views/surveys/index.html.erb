<ul class="center">
	<div class="row">
		<div class="small-12 columns centered">
			<h4 class="subheader">Surveys</h4>
		</div>
	</div>
	<br/>
	<br/>
	<% @surveys.each do |survey| %>
	<div class="row">
	    <div class="small-4 columns">
			<%= label_tag survey.survey_name,nil, class: 'left inline'%>
	    </div>
	    <div class="small-4 columns">
	    	<%= link_to "Take it", "/surveys/#{survey.id}/survey_answers/new", class: "button tiny"%>
	    	<%= link_to "Edit Survey", "/surveys/#{survey.id}/edit", class: "button tiny"%>
		</div>
		<div class="small-4 columns">
		</div>
	</div>
	<%end%>
</ul>

